{
  "name": "xtj",
  "version": "1.0.3",
  "packageManager": "pnpm@6.32.3",
  "description": "typescript-template",
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.19.4",
    "eslint": "8.13.0",
    "esno": "0.14.1",
    "happy-dom": "2.55.0",
    "np": "7.6.1",
    "typescript": "4.6.3",
    "unbuild": "0.7.4",
    "vite": "2.9.4",
    "vitest": "0.7.13"
  }
}